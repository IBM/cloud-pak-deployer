apiVersion: storage.k8s.io/v1
kind: StorageClass
metadata:
  name: managed-nfs-storage
allowVolumeExpansion: true
mountOptions:
- hard
- nfsvers=4.1
- sec=sys
parameters:
  billingType: hourly
  classVersion: "1"
  encrypted: "false"
  encryptionKey: ""
  gid: "0"
  iops: "100"
  isENIEnabled: "true"
  primaryIPAddress: ""
  primaryIPID: ""
  profile: dp2
  region: ""
  resourceGroup: ""
  securityGroupIDs: ""
  subnetID: ""
  tags: ""
  uid: "0"
  zone: ""
provisioner: vpc.file.csi.ibm.io
reclaimPolicy: Delete
volumeBindingMode: Immediate