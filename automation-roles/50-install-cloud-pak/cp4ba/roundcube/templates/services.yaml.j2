apiVersion: v1
kind: Service
metadata:
  name: roundcube
  namespace: "{{ roundcube_project_name }}"
  labels:
    app: roundcube
spec:
  ports:
  - port: 9000
    protocol: TCP
  selector:
    service: roundcube
---
apiVersion: v1
kind: Service
metadata:
  name: roundcube-nginx
  namespace: "{{ roundcube_project_name }}"
  labels:
    app: roundcube
spec:
  ports:
  - name: http
    port: 8080
    targetPort: 80
  selector:
    service: roundcube-nginx
