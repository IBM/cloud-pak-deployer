kind: ClusterRoleBinding
apiVersion: rbac.authorization.k8s.io/v1
metadata:
  name: 'phpldapadmin-anyuid'
  namespace: "{{ phpldapadmin_project_name }}"
  labels:
    app: phpldapadmin  
subjects:
  - kind: ServiceAccount
    name: phpldapadmin
    namespace: "{{ phpldapadmin_project_name }}"
roleRef:
  apiGroup: rbac.authorization.k8s.io
  kind: ClusterRole
  name: 'system:openshift:scc:anyuid'
