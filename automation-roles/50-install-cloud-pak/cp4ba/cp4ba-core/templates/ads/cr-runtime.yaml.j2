spec:
  datasource_configuration:
    dc_ads_runtime_datasource:
      dc_use_postgres: false
      database_type: postgresql
      database_servername: "{{ cp4ba_postgresql_hostname }}"
      database_port: 5432
      database_name: adsruntimedb
      current_schema: ads
      ssl_enabled: false
  ads_configuration:
    decision_runtime:
      enabled: true
      autoscaling:
        enabled: false
    decision_runtime_service:
      tls:
        allow_self_signed: true
      autoscaling:
        enabled: false
